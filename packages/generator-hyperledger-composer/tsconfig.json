{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "strictNullChecks": false,
    "baseUrl": "./generators",
    "paths": {
    },
    "lib": [
      "dom",
      "es6"
    ],
    "types": [
      "chai",
      "jasmine",
      "node",
      "sinon",
      "sinon-chai"
    ]
  },
  "exclude": [
    "node_modules",
    "coverage",
    "node_modules"
  ],
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "useWebpackText": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": { "rewriteTsconfig": false }
}
