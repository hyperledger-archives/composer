/**
 * Genesis model
 */
namespace org.hyperledger.composer.system

/**
 * Implicit super-type for all Assets
 */
abstract asset _cst_Asset {
   o String superNotes optional
}

/**
 * Implicit super-type for all Participants
 */
abstract participant _cst_Participant {
   o String superNotes optional
}

/**
 * Implicit super-type for all Transactions
 */
abstract transaction _cst_Transaction identified by transactionID {
  o String transactionID
  o DateTime timestamp
  o String superNotes optional
}

/**
 * Implicit super-type for all Events

   --> Transaction transaction
 */
abstract event _cst_Event identified by eventID {
  o String eventID
}
