{
  "name": "composer-connector-proxy",
  "version": "0.15.0",
  "description": "The proxying client connector for Hyperledger Composer",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha --recursive -t 10000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "eslint": "3.17.1",
    "jsdoc": "3.5.5",
    "license-check": "1.1.5",
    "mocha": "3.4.2",
    "nyc": "11.1.0",
    "proxyquire": "1.7.11",
    "serializerr": "1.0.3",
    "sinon": "2.3.8",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "composer-common": "0.15.0",
    "socket.io-client": "1.7.3"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
