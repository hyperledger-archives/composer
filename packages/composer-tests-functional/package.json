{
  "name": "composer-tests-functional",
  "version": "0.19.5",
  "private": true,
  "description": "Functional verification tests for Hyperledger Composer",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run licchk",
    "licchk": "license-check-and-add",
    "postlicchk": "npm run lint",
    "lint": "eslint .",
    "test": "exit 0",
    "systest:embedded": "mocha -t 0 systest",
    "systest:hlf": "mocha -t 0 systest",
    "systest:hlfv1": "mocha -t 0 systest",
    "systest:hlfv1_tls": "mocha -t 0 systest",
    "systest:hlfv1-1": "mocha -t 0 systest/[a-p]*.js systest/setup.js",
    "systest:hlfv1-2": "mocha -t 0 systest/[q-z]*.js systest/setup.js",
    "systest:hlfv1-1_tls": "mocha -t 0 systest/[a-p]*.js systest/setup.js",
    "systest:hlfv1-2_tls": "mocha -t 0 systest/[q-z]*.js systest/setup.js",
    "systest:hlfv1_hsm": "mocha -t 0 systest/historian.js systest/accesscontrols.js systest/identities.js systest/setup.js",
    "systest:proxy": "mocha -t 0 systest",
    "systest:web": "karma start --single-run",
    "start_http": "PM2_HOME=.pm2 pm2 start --wait-ready scripts/http.js",
    "stop_http": "PM2_HOME=.pm2 pm2 stop scripts/http.js || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer.git"
  },
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-polyfill": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "body-parser": "1.17.0",
    "brfs": "1.4.3",
    "browserfs": "1.1.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-subset": "1.3.0",
    "composer-admin": "0.19.5",
    "composer-client": "0.19.5",
    "composer-common": "0.19.5",
    "composer-connector-embedded": "0.19.5",
    "composer-connector-proxy": "0.19.5",
    "composer-connector-server": "0.19.5",
    "composer-connector-web": "0.19.5",
    "composer-wallet-inmemory": "0.19.5",
    "dockerode": "2.5.1",
    "eslint": "3.17.1",
    "express": "4.15.2",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-mocha": "1.3.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "2.0.2",
    "license-check-and-add": "2.3.1",
    "mkdirp": "0.5.1",
    "mocha": "3.4.2",
    "pm2": "2.10.1",
    "sleep-promise": "2.0.0",
    "socket.io": "1.7.3",
    "transform-loader": "0.2.4",
    "uuid": "3.0.1",
    "watchify": "3.7.0"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "node_modules",
      ".tern-project",
      ".pm2"
  
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".yaml",
      ".tgz",
      ".key",
      ".card",
      ".gz",
      ".pem",
      ".bna",
      ".crt",
      ".tx",
      ".block",
      ".log",
      ".pid",
      ".port"
    ],
    "insert_license": false,
    "license_formats": {
      "js|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "npmrc|editorconfig|dockerignore": {
        "eachLine": {
          "prepend": "# "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    }
  }
}
