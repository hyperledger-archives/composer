{
  "name": "composer-runtime-web",
  "version": "0.4.2",
  "description": "The web runtime container for Fabric Composer",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf -t ./node_modules/ink-docstrap/template",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-composer/fabric-composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Fabric Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.1.1",
    "browserify-istanbul": "^2.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.10.2",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.26",
    "license-check": "^1.1.5",
    "mocha": "^3.1.2",
    "moment": "^2.16.0",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "watchify": "^3.7.0"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {
    "composer-runtime": "^0.4.2",
    "dexie": "^1.5.1",
    "uuid": "^3.0.1"
  }
}
