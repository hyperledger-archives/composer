    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "basic-sample-network": "latest",
    "bootstrap": "4.0.0-alpha.5",
    "browserfs": "^1.1.0",
    "buffer-loader": "0.0.1",
    "chai": "^3.5.0",
    "codelyzer": "^2.0.1",
    "codemirror": "^5.22.0",
    "composer-admin": "^0.7.5",
    "composer-client": "^0.7.5",
    "composer-connector-proxy": "^0.7.5",
    "composer-connector-web": "^0.7.5",
    "composer-runtime": "^0.7.5",
    "composer-runtime-web": "^0.7.5",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "dexie": "^1.5.1",
    "doctrine": "^2.0.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "fast-json-patch": "^1.1.4",
    "file-loader": "^0.10.0",
    "gh-pages": "^0.12.0",
    "homedir": "^0.6.0",
    "html-webpack-plugin": "^2.25.0",
    "ie-shim": "^0.1.0",
    "imports-loader": "^0.7.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "jsonata": "^1.2.2",
    "jszip": "^3.1.3",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
