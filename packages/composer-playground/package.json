    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "left-pad": "^1.1.3",
    "lz-string": "^1.4.4",
    "mocha": "^3.2.0",
