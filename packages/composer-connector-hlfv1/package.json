{
  "name": "composer-connector-hlfv1",
  "version": "0.4.5",
  "description": "The Hyperledger Fabric v1.x Client connector for Fabric Composer",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf -t ./node_modules/ink-docstrap/template",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "mocha --recursive && istanbul cover --include-all-sources --report cobertura --report html ./node_modules/mocha/bin/_mocha -- --recursive",
    "posttest": "istanbul check-coverage",
    "postinstall": "cd node_modules/fabric-sdk-node && npm install && gulp ca && cd ../.. && node copysdk.js && cd node_modules/fabric-client && npm install && cd ../fabric-ca-client && npm install && npm install grpc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-composer/fabric-composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Fabric Composer",
  "license": "Apache-2.0",
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {
    "composer-common": "^0.4.5",
    "composer-runtime-hlfv1": "^0.4.5",
    "fabric-sdk-node": "davidkel/fabric-sdk-node#Tested020317",
    "fs-extra": "^1.0.0",
    "semver": "^5.3.0",
    "temp": "^0.8.3",
    "thenify-all": "^1.6.0",
    "uuid": "^3.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.10.2",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "license-check": "^1.1.5",
    "mocha": "^3.1.2",
    "moment": "^2.16.0",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "gulp": "3.9.1"
  }
}
