{
  "name": "composer-rest-server",
  "version": "0.20.3",
  "description": "Hyperledger Composer REST server that uses the Hyperledger Composer LoopBack connector",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "index.js",
  "bin": {
    "composer-rest-server": "cli.js"
  },
  "scripts": {
    "start": "node .",
    "pretest": "npm run licchk",
    "licchk": "license-check-and-add",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha --recursive -t 10000",
    "mocha": "mocha --recursive -t 10000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "1.17.0",
    "clui": "0.3.1",
    "composer-admin": "0.20.3",
    "composer-common": "0.20.3",
    "composer-wallet-filesystem": "0.20.3",
    "composer-wallet-inmemory": "0.20.3",
    "compression": "1.7.3",
    "connect-ensure-login": "0.1.1",
    "cookie-parser": "1.4.3",
    "cors": "2.5.2",
    "express-flash": "0.0.2",
    "express-session": "1.15.1",
    "formidable": "1.1.1",
    "helmet": "1.3.0",
    "inquirer": "3.3.0",
    "lodash": "4.17.11",
    "loopback": "3.4.0",
    "loopback-boot": "2.25.0",
    "loopback-component-explorer": "5.2.0",
    "loopback-component-passport": "3.2.0",
    "loopback-connector-composer": "0.20.3",
    "passport-local": "1.0.0",
    "pug": "2.0.0",
    "serve-favicon": "2.5.0",
    "strong-error-handler": "1.0.1",
    "touch": "1.0.0",
    "ws": "3.3.3",
    "yargs": "10.0.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-http": "3.0.0",
    "clone": "2.1.1",
    "composer-client": "0.20.3",
    "composer-connector-embedded": "0.20.3",
    "eslint": "3.17.1",
    "jsdoc": "3.5.5",
    "ldapjs": "1.0.1",
    "license-check-and-add": "2.3.1",
    "mocha": "3.4.2",
    "nyc": "11.1.0",
    "passport-ldapauth": "2.0.0",
    "proxyquire": "1.7.11",
    "sinon": "2.3.8",
    "sleep-promise": "2.0.0"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "coverage",
      "node_modules",
      ".nyc-output",
      "out",
      "composer-logs",
      "cli.js",
      "public/lib/loadSwaggerUI.js"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".pem"
    ],
    "insert_license": false,
    "default_format": {
      "eachLine": {
        "prepend": "# "
      }
    },
    "license_formats": {
      "js|qry|acl|cto|css": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "npmrc|editorconfig|ppc64Ie": {
        "eachLine": {
          "prepend": "# "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    }
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "public/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
