{
  "name": "composer-rest-server",
  "version": "0.4.5",
  "description": "Fabric Composer REST server that uses the Fabric Composer LoopBack connector",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "bin": {
    "composer-rest-server": "server/server.js"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "node .",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "posttest": "echo \"Warning: no tests no coverage check\" && exit 0 && istanbul check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-composer/fabric-composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Fabric Composer",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "figlet": "^1.2.0",
    "helmet": "^1.3.0",
    "inquirer": "^3.0.1",
    "lodash": "^4.17.4",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.1.0",
    "loopback-connector-composer": "^0.4.5",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^1.0.1",
    "touch": "^1.0.0",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.2",
    "license-check": "^1.1.5",
    "loopback": "^2.35.0",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6",
    "sleep-promise": "^2.0.0"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./server/server.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  }
}
