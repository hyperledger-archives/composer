{
  "name": "composer-rest-server",
  "version": "0.7.5",
  "description": "Hyperledger Composer REST server that uses the Hyperledger Composer LoopBack connector",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "main": "index.js",
  "bin": {
    "composer-rest-server": "cli.js"
  },
  "scripts": {
    "start": "node .",
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha --recursive -t 5000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.17.0",
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "composer-common": "^0.7.5",
    "compression": "^1.0.3",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.5.2",
    "express-flash": "0.0.2",
    "express-session": "^1.15.1",
    "figlet": "^1.2.0",
    "helmet": "^1.3.0",
    "inquirer": "^3.0.1",
    "jade": "^1.11.0",
    "lodash": "^4.17.4",
    "loopback": "^3.4.0",
    "loopback-boot": "^2.23.0",
    "loopback-component-explorer": "^4.1.1",
    "loopback-component-passport": "^3.2.0",
    "loopback-connector-composer": "^0.7.5",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^1.0.1",
    "touch": "^1.0.0",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "clone": "^2.1.1",
    "composer-admin": "^0.7.5",
    "composer-client": "^0.7.5",
    "composer-connector-embedded": "^0.7.5",
    "eslint": "^3.17.1",
    "jsdoc": "^3.4.3",
    "license-check": "^1.1.5",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "passport-github2": "^0.1.10",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2",
    "sleep-promise": "^2.0.0"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./cli.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
