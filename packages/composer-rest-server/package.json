{
  "name": "composer-rest-server",
  "version": "0.15.0",
  "description": "Hyperledger Composer REST server that uses the Hyperledger Composer LoopBack connector",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "index.js",
  "bin": {
    "composer-rest-server": "cli.js"
  },
  "scripts": {
    "start": "node .",
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha --recursive -t 10000",
    "mocha": "mocha --recursive -t 10000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "1.17.0",
    "clui": "0.3.1",
    "composer-admin": "0.15.0",
    "composer-common": "0.15.0",
    "compression": "1.0.3",
    "connect-ensure-login": "0.1.1",
    "cookie-parser": "1.4.3",
    "cors": "2.5.2",
    "express-flash": "0.0.2",
    "express-session": "1.15.1",
    "formidable": "1.1.1",
    "helmet": "1.3.0",
    "inquirer": "3.0.1",
    "jade": "1.11.0",
    "lodash": "4.17.4",
    "loopback": "3.4.0",
    "loopback-boot": "2.25.0",
    "loopback-component-explorer": "5.2.0",
    "loopback-component-passport": "3.2.0",
    "loopback-connector-composer": "0.15.0",
    "passport-local": "1.0.0",
    "serve-favicon": "2.0.1",
    "strong-error-handler": "1.0.1",
    "touch": "1.0.0",
    "ws": "3.0.0",
    "yargs": "8.0.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-http": "3.0.0",
    "clone": "2.1.1",
    "composer-client": "0.15.0",
    "composer-connector-embedded": "0.15.0",
    "eslint": "3.17.1",
    "jsdoc": "3.5.5",
    "ldapjs": "1.0.1",
    "license-check": "1.1.5",
    "mocha": "3.4.2",
    "nyc": "11.1.0",
    "passport-ldapauth": "2.0.0",
    "proxyquire": "1.7.11",
    "sinon": "2.3.8",
    "sleep-promise": "2.0.0"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./cli.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "public/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
