{
  "name": "composer-connector-server",
  "version": "0.5.5",
  "description": "The remote connector server for Fabric Composer",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "bin": {
    "composer-connector-server": "cli.js"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-composer/fabric-composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Fabric Composer",
  "license": "Apache-2.0",
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./cli.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {
    "composer-common": "^0.5.5",
    "composer-connector-embedded": "^0.5.5",
    "composer-connector-hlf": "^0.5.5",
    "serializerr": "^1.0.3",
    "socket.io": "^1.7.3",
    "uuid": "^3.0.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "jsdoc": "^3.4.3",
    "license-check": "^1.1.5",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 2,
    "branches": 0,
    "functions": 0,
    "lines": 2
  }
}
