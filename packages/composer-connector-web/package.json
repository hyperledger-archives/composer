{
  "name": "composer-connector-web",
  "version": "0.7.5",
  "description": "The web client connector for Hyperledger Composer",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prepublish": "node ./scripts/tsgen.js",
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.3.0",
    "browserify-istanbul": "^2.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.17.1",
    "jsdoc": "^3.4.3",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.26",
    "license-check": "^1.1.5",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "composer-common": "^0.7.5",
    "composer-runtime": "^0.7.5",
    "composer-runtime-web": "^0.7.5",
    "uuid": "^3.0.1"
  }
}
