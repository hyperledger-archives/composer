{
  "name": "composer-systests",
  "version": "0.4.5",
  "private": true,
  "description": "System tests and automation for Fabric Composer",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run lint",
    "lint": "eslint .",
    "test": "exit 0",
    "systest:embedded": "mocha -t 0 systest",
    "systest:hlf": "mocha -t 0 systest",
    "systest:web": "karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-composer/fabric-composer.git"
  },
  "author": "Fabric Composer",
  "license": "Apache-2.0",
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserfs": "^1.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-subset": "^1.3.0",
    "composer-admin": "^0.4.5",
    "composer-client": "^0.4.5",
    "composer-common": "^0.4.5",
    "composer-connector-embedded": "^0.4.5",
    "composer-connector-web": "^0.4.5",
    "eslint": "^3.10.2",
    "homedir": "^0.6.0",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.26",
    "license-check": "^1.1.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "sleep-promise": "^2.0.0",
    "uuid": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./systest/data/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {}
}
