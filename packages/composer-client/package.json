{
  "name": "composer-client",
  "version": "0.7.5",
  "description": "The node.js client library for Hyperledger Composer, a development framework for Hyperledger Fabric",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prepublish": "node ./scripts/tsgen.js",
    "pretest": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf",
    "postdoc": "npm run lint",
    "lint": "eslint .",
    "test": "node ./scripts/api-changelog.js && nyc mocha --recursive -t 5000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {
    "composer-common": "^0.7.5",
    "composer-connector-hlf": "^0.7.5",
    "composer-connector-hlfv1": "^0.7.5",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-things": "^0.2.0",
    "eslint": "^3.17.1",
    "jsdoc": "^3.4.3",
    "license-check": "^1.1.5",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
